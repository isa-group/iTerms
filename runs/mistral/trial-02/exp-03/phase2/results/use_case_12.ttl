@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application will avoid excessive and unnecessary use of the API Service that may cause a performance degradation of the API Service." ;
    odrl:action odrl:use ;
    odrl:target :apiService ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:count ;
        odrl:operator odrl:lt ;
        odrl:rightOperand "1000"^^xsd:integer ;
        odrl:unit <http://www.w3.org/2006/time#minute>
    ] .