@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "API service is provided on an 'as is' and 'as available' basis without warranties of any kind."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is a global information analytics business that helps institutions and professionals progress science, advance healthcare, and improve performance."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiService .

:apiService a tosl:Service ;
    dcterms:description "API service provided by ELSEVIER." .

:permission01 a odrl:Permission ;
    dcterms:description "API service is provided on an 'as is' and 'as available' basis without warranties of any kind." ;
    odrl:action odrl:use ;
    odrl:target :apiService ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:purpose ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "as is"^^xsd:string
    ] ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:purpose ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "as available"^^xsd:string
    ] .

:customer a tosl:Customer ;
    dcterms:description "This party represents the consumer in the context of the ELSEVIER API service agreement." .