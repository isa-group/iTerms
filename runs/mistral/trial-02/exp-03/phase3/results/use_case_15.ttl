@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:permission01 a odrl:Permission ;
    dcterms:description "Developer shall obtain written permission in advance from Elsevier to use Elsevier's trademarks, logos or other brand features for promoting the Developer's Application." ;
    odrl:action tosl:consent ;
    odrl:target :trademarks ;
    odrl:assignee :Developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :promotion
    ] ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Developer must inform Elsevier about the use of trademarks for promotion." ;
        odrl:action odrl:inform ;
        odrl:target :trademarks ;
        odrl:assignee :Elsevier ;
        odrl:informedParty :Developer
    ] .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "Any press releases regarding this Agreement or the relationship of the parties shall require the prior approval of the parties." ;
    odrl:action odrl:inform ;
    odrl:target :pressReleases ;
    odrl:assignee :Developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :pressRelease
    ] ;
    odrl:remedy [
        a odrl:Duty ;
        dcterms:description "Developer must obtain consent from Elsevier for press releases." ;
        odrl:action odrl:obtainConsent ;
        odrl:target :pressReleases ;
        odrl:assignee :Developer ;
        odrl:informedParty :Elsevier
    ] .

:trademarks a tosl:Service ;
    dcterms:description "Elsevier's trademarks, logos or other brand features." .

:pressReleases a tosl:Service ;
    dcterms:description "Press releases regarding this Agreement or the relationship of the parties." .

:promotion a odrl:Event ;
    dcterms:description "Promoting the Developer's Application." .

:pressRelease a odrl:Event ;
    dcterms:description "Press releases regarding this Agreement or the relationship of the parties." .

:Developer a tosl:Customer ;
    dcterms:description "The Developer using Elsevier's API." .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier, the provider of the API." .