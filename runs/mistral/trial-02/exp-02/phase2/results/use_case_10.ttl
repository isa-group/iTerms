@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application will not use Elsevier content other than allowed by the API's Content Usage Policies on http://dev.elsevier.com/policy.html." ;
    odrl:action odrl:use ;
    odrl:target :elsevierContent ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:compliance ;
        odrl:operator odrl:neq ;
        odrl:rightOperand <http://dev.elsevier.com/policy.html>
    ] .

:elsevierContent a tosl:Content ;
    dcterms:description "Elsevier content accessible through the API." .

:customer a tosl:Customer ;
    dcterms:description "This party represents the customer in the context of the Elsevier API Service Agreement." .