@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Developer acknowledges that all right, title and interest in and to the Elsevier content, and any derivative works based upon the Elsevier content, remain with Elsevier and its suppliers, except as expressly set forth in this Agreement, and that the unauthorized redistribution of the Elsevier content is not permitted." ;
    odrl:action odrl:distribute ;
    odrl:target :elsevierContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:purpose ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :authorizedRedistribution
    ] .

:elsevierContent a tosl:Content ;
    dcterms:description "Elsevier content as provided through the API." .

:developer a tosl:Customer ;
    dcterms:description "The Developer using the Elsevier API." .

:authorizedRedistribution a odrl:Purpose ;
    dcterms:description "Redistribution authorized by the Agreement." .