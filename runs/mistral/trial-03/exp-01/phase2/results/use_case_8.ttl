@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:apiServiceAgreement a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "API SERVICE AGREEMENT"^^xsd:string;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :ELSEVIER .

:apiService a tosl:Service ;
    dcterms:description "API service provided by ELSEVIER." .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is a global information analytics business that helps institutions and professionals progress science, advance healthcare, and improve performance."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiServiceAgreement .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "In a situation of breach of the terms and conditions of this Agreement, Elsevier reserves the right to block, change, suspend, remove or disable access to the APIs and any of its services at any time upon reasonable notice to Developer." ;
    odrl:action tosl:terminate ;
    odrl:target :apiService ;
    odrl:assignee :ELSEVIER ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Upon reasonable notice to Developer." ;
        odrl:leftOperand odrl:delayPeriod ;
        odrl:operator odrl:gteq ;
        odrl:rightOperand "P0D"^^xsd:duration
    ] .