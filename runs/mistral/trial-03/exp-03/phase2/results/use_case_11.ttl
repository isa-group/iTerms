@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application will use the API Key issued by Elsevier upon execution of this Agreement, and no other API Key issued for use by other applications and/or to other developers." ;
    odrl:action odrl:use ;
    odrl:target :apiKey ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand :apiKey ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :elsevierIssuedApiKey
    ] .

:apiKey a odrl:Asset ;
    dcterms:description "API Key issued by Elsevier for use with their services." .

:application a tosl:Customer ;
    dcterms:description "The application using the Elsevier API." .

:elsevierIssuedApiKey a odrl:Asset ;
    dcterms:description "API Key issued by Elsevier upon execution of the Agreement." .