@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Developer shall not display any advertising inside the Application or use any content as obtained via the API outside the Application, unless permitted by Elsevier in writing." ;
    odrl:action odrl:display ;
    odrl:target :apiContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :elsevierPermission
    ] .

:apiContent a odrl:Asset ;
    dcterms:description "Content obtained via the Elsevier API." .

:developer a tosl:Customer ;
    dcterms:description "The Developer using the Elsevier API." .

:elsevierPermission a odrl:Asset ;
    dcterms:description "Written permission from Elsevier." .