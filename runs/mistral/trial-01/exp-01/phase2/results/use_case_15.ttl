@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:obligation01 a odrl:Duty ;
    dcterms:description "Developer shall obtain written permission in advance from Elsevier to use Elsevier's trademarks, logos or other brand features for promoting the Developer's Application. Any press releases regarding this Agreement or the relationship of the parties shall require the prior approval of the parties." ;
    odrl:action odrl:obtainConsent ;
    odrl:target :trademarks ;
    odrl:assignee :developer ;
    odrl:consentingParty :elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:consentType ;
        odrl:operator odrl:eq ;
        odrl:rightOperand tosl:writtenPermission
    ] .

:trademarks a odrl:Asset ;
    dcterms:description "Elsevier's trademarks, logos or other brand features" .

:developer a tosl:BusinessCustomer ;
    dcterms:description "The developer of the application using Elsevier's API" .

:elsevier a tosl:Provider ;
    dcterms:description "Elsevier, the provider of the API service" .