@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:apiService a tosl:Service ;
    dcterms:description "API services provided by Elsevier." .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information and analytics company specializing in scientific, technical, and medical content."^^xsd:string ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiService .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "In a situation of breach of the terms and conditions of this Agreement, Elsevier reserves the right to block, change, suspend, remove or disable access to the APIs and any of its services at any time upon reasonable notice to Developer." ;
    odrl:action tosl:terminate ;
    odrl:target :apiService ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :breachEvent
    ] .

:breachEvent a odrl:Event ;
    dcterms:description "Breach of the terms and conditions of the Agreement." .