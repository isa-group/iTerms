@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER" .

:customer a tosl:Customer ;
    dcterms:description "This party represents the consumer in the context of the Elsevier API Service Agreement." .

:agreement a odrl:Asset ;
    dcterms:title "API SERVICE AGREEMENT" ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:issued "2017"^^xsd:gYear .

:obligation_accept_terms a odrl:Duty ;
    dcterms:description "You accept and agree to the terms and conditions of this Agreement and the terms and conditions of the agreement between Elsevier and Your institution, company or organization or if not applicable, as agreed between Elsevier and You for Your specific use case." ;
    odrl:action tosl:consent ;
    odrl:target :agreement ;
    odrl:assignee :customer .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement for use of Elsevier API services."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligation_accept_terms ;
    odrl:assigner :ELSEVIER .