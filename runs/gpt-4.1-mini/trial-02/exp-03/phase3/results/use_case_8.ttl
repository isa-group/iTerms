@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:APIService a tosl:Service ;
    dcterms:description "APIs and any of its services provided by Elsevier." .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API services." ;
    foaf:name "Elsevier" .

:Developer a tosl:Customer ;
    dcterms:description "Developer who uses the Elsevier APIs and services." .

:permissionBlockChangeSuspendRemoveDisable a odrl:Permission ;
    dcterms:description "Elsevier may block, change, suspend, remove or disable access to the APIs and any of its services at any time upon reasonable notice to Developer in case of breach of the terms and conditions of this Agreement." ;
    odrl:action odrl:block, odrl:modify, odrl:remove ;
    odrl:target :APIService ;
    odrl:assignee :Elsevier ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Elsevier must provide reasonable notice to Developer before exercising these rights." ;
        odrl:action odrl:inform ;
        odrl:target :APIService ;
        odrl:assignee :Elsevier
    ] .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement granting Elsevier the right to block, change, suspend, remove or disable access to the APIs and services upon breach with reasonable notice to Developer."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permissionBlockChangeSuspendRemoveDisable ;
    odrl:assigner :Elsevier .