@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API service and content." .

:elsevierContent a odrl:Asset ;
    dcterms:description "Content provided by Elsevier accessible via the API." .

:apiContentUsagePolicies a odrl:Asset ;
    dcterms:description "Elsevier API Content Usage Policies available at http://dev.elsevier.com/policy.html" .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application will not use Elsevier content other than allowed by the API's Content Usage Policies on http://dev.elsevier.com/policy.html." ;
    odrl:action odrl:use ;
    odrl:target :elsevierContent ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Usage must comply with the API's Content Usage Policies." ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :apiContentUsagePolicies
    ] .

:application a tosl:Customer ;
    dcterms:description "The Application using Elsevier content via the API." .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing the use of Elsevier content via API, including compliance with Content Usage Policies."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :Elsevier .