@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing the use of the API Key issued by Elsevier for the Application, restricting use to the issued API Key only."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the provider issuing API Keys for authorized applications."^^xsd:string .

:applicationAPI a odrl:Asset ;
    dcterms:description "The Application using the API Key issued by Elsevier."^^xsd:string .

:apiKeyIssued a odrl:Asset ;
    dcterms:description "The API Key issued by Elsevier to the Application upon execution of this Agreement."^^xsd:string .

:permission01 a odrl:Permission ;
    dcterms:description "The Application is permitted to use only the API Key issued by Elsevier upon execution of this Agreement."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiKeyIssued ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "The API Key used must be the one issued to this Application and not any other API Key issued to other applications or developers."^^xsd:string ;
        odrl:leftOperand tosl:licensingType ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :issuedAPIKeyForThisApplication
    ] .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application is prohibited from using any API Key other than the one issued by Elsevier for this Application."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiKeyIssued ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "No use of API Keys issued for other applications or developers is allowed."^^xsd:string ;
        odrl:leftOperand tosl:licensingType ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :issuedAPIKeyForOtherApplications
    ] .

:application a tosl:Customer ;
    dcterms:description "The Application authorized to use the API Key issued by Elsevier."^^xsd:string .

:issuedAPIKeyForThisApplication a odrl:Asset ;
    dcterms:description "The specific API Key issued to this Application."^^xsd:string .

:issuedAPIKeyForOtherApplications a odrl:Asset ;
    dcterms:description "API Keys issued to other applications or developers."^^xsd:string .