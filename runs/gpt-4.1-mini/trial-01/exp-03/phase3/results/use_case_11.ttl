@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing the use of the API Key issued by Elsevier for the Application."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the provider issuing API Keys for authorized applications."^^xsd:string .

:applicationAPIKey a odrl:Asset ;
    dcterms:description "The API Key issued by Elsevier for use by the Application under this Agreement."^^xsd:string .

:otherAPIKeys a odrl:Asset ;
    dcterms:description "API Keys issued by Elsevier for use by other applications and/or other developers."^^xsd:string .

:permission01 a odrl:Permission ;
    dcterms:description "The Application is permitted to use only the API Key issued by Elsevier upon execution of this Agreement."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :applicationAPIKey ;
    odrl:assignee :application .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application is prohibited from using any API Key issued for other applications and/or other developers."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :otherAPIKeys ;
    odrl:assignee :application .

:application a tosl:Customer ;
    dcterms:description "The Application authorized to use the API Key issued by Elsevier under this Agreement."^^xsd:string .