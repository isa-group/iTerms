@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:APIServiceAgreement a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.1.1: The Application will not use Elsevier content other than allowed by the API's Content Usage Policies on http://dev.elsevier.com/policy.html."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:date ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is the provider of the API service and content."^^xsd:string .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application is prohibited from using Elsevier content except as allowed by the API's Content Usage Policies available at http://dev.elsevier.com/policy.html."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :ElsevierContent ;
    odrl:assignee :Application ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Usage must comply with the API's Content Usage Policies at http://dev.elsevier.com/policy.html"^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand <http://dev.elsevier.com/policy.html>
    ] .

:ElsevierContent a odrl:Asset ;
    dcterms:description "Content provided by Elsevier accessible via the API."^^xsd:string .

:Application a tosl:Customer ;
    dcterms:description "The Application using Elsevier content via the API."^^xsd:string .