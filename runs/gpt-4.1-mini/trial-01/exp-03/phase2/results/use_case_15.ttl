@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API service." .

:Developer a tosl:Customer ;
    dcterms:description "Developer who uses Elsevier's API services." .

:trademarksLogosBrandFeatures a odrl:Asset ;
    dcterms:description "Elsevier's trademarks, logos or other brand features." .

:pressReleases a odrl:Asset ;
    dcterms:description "Press releases regarding this Agreement or the relationship of the parties." .

:permissionUseBrandFeatures a odrl:Permission ;
    dcterms:description "Developer shall obtain written permission in advance from Elsevier to use Elsevier's trademarks, logos or other brand features for promoting the Developer's Application." ;
    odrl:action odrl:use ;
    odrl:target :trademarksLogosBrandFeatures ;
    odrl:assignee :Developer ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Developer must obtain written permission in advance from Elsevier before using trademarks, logos or other brand features." ;
        odrl:action odrl:obtainConsent ;
        odrl:target :trademarksLogosBrandFeatures ;
        odrl:assignee :Developer
    ] .

:permissionPressReleases a odrl:Permission ;
    dcterms:description "Any press releases regarding this Agreement or the relationship of the parties shall require the prior approval of the parties." ;
    odrl:action odrl:publish ;
    odrl:target :pressReleases ;
    odrl:assignee :Developer ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Developer must obtain prior approval of the parties before issuing any press releases regarding this Agreement or the relationship of the parties." ;
        odrl:action odrl:obtainConsent ;
        odrl:target :pressReleases ;
        odrl:assignee :Developer
    ] .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement covering permissions related to use of Elsevier's trademarks and press releases."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permissionUseBrandFeatures, :permissionPressReleases ;
    odrl:assigner :Elsevier .