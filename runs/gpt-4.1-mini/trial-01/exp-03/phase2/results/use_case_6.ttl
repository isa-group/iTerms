@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier grants a limited license to use its APIs and related materials to develop and integrate your application, subject to usage policies, with all notices delivered in writing to the email listed in the developer portal."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is the provider of the API services and related materials."^^xsd:string .

:APIs a odrl:Asset ;
    dcterms:description "APIs and related materials provided by ELSEVIER."^^xsd:string .

:permission01 a odrl:Permission ;
    dcterms:description "You are granted a limited license to use ELSEVIER APIs and related materials to develop and integrate your application, subject to usage policies."^^xsd:string ;
    odrl:action tosl:develop, tosl:integrate ;
    odrl:target :APIs ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Use is subject to usage policies."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :usagePolicies
    ] .

:usagePolicies a odrl:Asset ;
    dcterms:description "Usage policies applicable to the use of ELSEVIER APIs."^^xsd:string .

:obligation01 a odrl:Duty ;
    dcterms:description "All notices must be delivered in writing to the email listed in the developer portal."^^xsd:string ;
    odrl:action odrl:inform ;
    odrl:target :APIs ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Notices must be delivered in writing to the developer portal email."^^xsd:string ;
        odrl:leftOperand tosl:justification ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :developerPortalEmail
    ] .

:customer a tosl:Customer ;
    dcterms:description "The party authorized to use ELSEVIER APIs under this agreement."^^xsd:string .

:developerPortalEmail a odrl:Asset ;
    dcterms:description "The email address listed in the developer portal for receiving notices."^^xsd:string .