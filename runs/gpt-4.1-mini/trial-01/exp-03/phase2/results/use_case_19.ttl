@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "The term of this Agreement shall commence on the date on which the Developer has accepted this Agreement and shall continue until either party terminates the Agreement in writing or the API Key is deactivated by Elsevier."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:obligation :obligation01, :obligation02 ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is the provider of the API service."^^xsd:string ;
    foaf:name "ELSEVIER" .

:Developer a tosl:Customer ;
    dcterms:description "Developer who accepts the Agreement."^^xsd:string ;
    foaf:name "Developer" .

:permission01 a odrl:Permission ;
    dcterms:description "Developer is permitted to use the API service from the date of acceptance of this Agreement until termination or API Key deactivation."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :agreement01 ;
    odrl:assignee :Developer ;
    odrl:duty :obligation01, :obligation02 ;
    odrl:prohibition :prohibition01 .

:obligation01 a odrl:Duty ;
    dcterms:description "Developer must accept this Agreement to commence the term."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :Developer .

:obligation02 a odrl:Duty ;
    dcterms:description "Either party must provide written notice to terminate the Agreement."^^xsd:string ;
    odrl:action odrl:inform ;
    odrl:target :agreement01 ;
    odrl:assignee :Developer .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The API Key must not be used after deactivation by Elsevier."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :agreement01 ;
    odrl:assignee :Developer .