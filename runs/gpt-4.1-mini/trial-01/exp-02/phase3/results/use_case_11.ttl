@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:APIServiceAgreement a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement requiring the Application to use only the API Key issued by Elsevier upon execution of this Agreement."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligationUseIssuedAPIKey ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the provider issuing API Keys for use with their API services."^^xsd:string .

:apiKey a odrl:Asset ;
    dcterms:description "API Key issued by Elsevier for use with the Application."^^xsd:string .

:obligationUseIssuedAPIKey a odrl:Duty ;
    dcterms:description "The Application must use only the API Key issued by Elsevier upon execution of this Agreement, and no other API Key issued for use by other applications and/or to other developers."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiKey ;
    odrl:assignee :ELSEVIER .