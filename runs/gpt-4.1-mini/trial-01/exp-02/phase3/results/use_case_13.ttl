@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:elsevierContent a odrl:Asset ;
    dcterms:title "Elsevier Content"^^xsd:string ;
    dcterms:description "Content provided by Elsevier and its suppliers, including derivative works based upon the Elsevier content."^^xsd:string .

:customer a tosl:Customer ;
    dcterms:description "The party acting as the developer or user of Elsevier content under this agreement."^^xsd:string .

:OpenAI a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the content and services under this agreement."^^xsd:string ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://dev.elsevier.com/> .

:obligation01 a odrl:Duty ;
    dcterms:description "The Developer acknowledges that all right, title and interest in and to the Elsevier content, and any derivative works based upon the Elsevier content, remain with Elsevier and its suppliers, except as expressly set forth in this Agreement, and that the unauthorized redistribution of the Elsevier content is not permitted."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :elsevierContent ;
    odrl:assignee :customer .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing the use of Elsevier content and API services."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :OpenAI ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligation01 ;
    odrl:assigner :OpenAI .