@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API services and related materials." .

:APIs a odrl:Asset ;
    dcterms:description "APIs and related materials provided by Elsevier for development and integration." .

:developerPortalEmail a odrl:Asset ;
    dcterms:description "Email address listed in the developer portal for delivery of all notices." .

:usagePolicies a odrl:Asset ;
    dcterms:description "Usage policies applicable to the use of Elsevier APIs." .

:customer a tosl:Customer ;
    dcterms:description "This party represents the consumer or developer using the Elsevier APIs." .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier grants a limited license to use its APIs and related materials to develop and integrate your application, subject to usage policies, with all notices delivered in writing to the email listed in the developer portal." ;
    odrl:action tosl:develop, tosl:integrate ;
    odrl:target :APIs ;
    odrl:assignee :customer ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "All notices must be delivered in writing to the email listed in the developer portal." ;
        odrl:action odrl:inform ;
        odrl:target :developerPortalEmail ;
        odrl:assignee :customer
    ] ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Use is subject to usage policies." ;
        odrl:leftOperand odrl:purpose ;
        odrl:operator odrl:isAnyOf ;
        odrl:rightOperand ( :usagePolicies )
    ] .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement granting limited license to use Elsevier APIs and related materials subject to usage policies and notice requirements."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .