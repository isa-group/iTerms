@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing the use of the API Key issued by Elsevier for the Application."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API service and issuer of API Keys."^^xsd:string .

:application a tosl:Customer ;
    dcterms:description "The Application using the API Key issued by Elsevier."^^xsd:string .

:apiKey a odrl:Asset ;
    dcterms:description "The API Key issued by Elsevier upon execution of this Agreement for use by the Application."^^xsd:string .

:permission01 a odrl:Permission ;
    dcterms:description "The Application is permitted to use only the API Key issued by Elsevier upon execution of this Agreement, and no other API Key issued for use by other applications or developers."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiKey ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "API Key must be the one issued specifically for this Application under this Agreement."^^xsd:string ;
        odrl:leftOperand tosl:licensingType ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :issuedAPIKeyForThisApplication
    ] .