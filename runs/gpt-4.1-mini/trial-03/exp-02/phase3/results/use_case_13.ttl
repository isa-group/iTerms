@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:elsevierContent a odrl:Asset ;
    dcterms:title "Elsevier Content"^^xsd:string ;
    dcterms:description "Content provided by Elsevier and its suppliers, including derivative works."^^xsd:string .

:customer a tosl:Customer ;
    dcterms:description "The party acting as the developer or user of Elsevier content." .

:obligation01 a odrl:Duty ;
    dcterms:description "The Developer acknowledges that all rights, title and interest in and to the Elsevier content and any derivative works remain with Elsevier and its suppliers, and that unauthorized redistribution of the Elsevier content is not permitted."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :elsevierContent ;
    odrl:assignee :customer .
    
:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing the use of Elsevier content and related services."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the provider and rights holder of the content and services."^^xsd:string .