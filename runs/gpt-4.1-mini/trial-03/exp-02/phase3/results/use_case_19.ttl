:obligation02 a odrl:Duty ;
    dcterms:description "Both parties are obliged to terminate the Agreement in writing or deactivate the API Key to end the term of the Agreement." ;
    odrl:action tosl:terminate ;
    odrl:target :agreement ;
    odrl:assignee :developer .

:developer a tosl:Customer ;
    dcterms:description "Developer who accepts the Agreement and uses the API." .