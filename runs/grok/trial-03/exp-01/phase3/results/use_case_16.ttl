@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "API Service Agreement clause 3.4."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :elsevier .

:elsevier a tosl:Provider ;
    foaf:name "ELSEVIER" .

:developer a tosl:Customer ;
    dcterms:description "The Developer" .

:apiContent a odrl:Asset ;
    dcterms:description "content as obtained via the API" .

:application a tosl:Service ;
    dcterms:description "The Application" .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Developer shall not display any advertising inside the Application or use any content as obtained via the API outside the Application, unless permitted by Elsevier in writing." ;
    odrl:action odrl:use ;
    odrl:target :apiContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:virtualLocation ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :application
    ], [
        a odrl:Constraint ;
        odrl:leftOperand tosl:justification ;
        odrl:operator odrl:neq ;
        odrl:rightOperand tosl:writtenPermission
    ] .