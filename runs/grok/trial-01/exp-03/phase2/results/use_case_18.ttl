@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    foaf:name "Elsevier" .

:customer a tosl:Customer .

:apiService a tosl:Service .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "API Service Agreement for Elsevier services."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    tosl:limitationOfLiability :liability01, :liability02 ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "Permission to use the API service subject to the agreement terms."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiService ;
    odrl:assignee :customer .

:liability01 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, each party disclaims all liability to the other party in connection with the parties' performance of this Agreement."^^xsd:string ;
    tosl:liableParty :Elsevier ;
    tosl:targetParty :customer ;
    rdf:value tosl:anyLiability .

:liability02 a tosl:Liability ;
    dcterms:description "In no event will the infringing party be liable to the other party for special, indirect or consequential damages including but not limited to loss of profits, loss of business or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :customer ;
    tosl:targetParty :Elsevier ;
    rdf:value tosl:anyIndirectDamage, tosl:breachOfContract .