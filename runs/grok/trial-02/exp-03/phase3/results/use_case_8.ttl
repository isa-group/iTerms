@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider .

:Developer a tosl:BusinessCustomer .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs and services" .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 2.2 In a situation of breach of the terms and conditions of this Agreement, Elsevier reserves the right to block, change, suspend, remove or disable access to the APIs and any of its services at any time upon reasonable notice to Developer."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "In a situation of breach of the terms and conditions of this Agreement, Elsevier reserves the right to block, change, suspend, remove or disable access to the APIs and any of its services at any time upon reasonable notice to Developer." ;
    odrl:action odrl:modify ;
    odrl:target :apis ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                dcterms:description "Upon breach of terms" ;
                odrl:leftOperand tosl:compliance ;
                odrl:operator odrl:neq ;
                odrl:rightOperand "true"^^xsd:boolean
            ]
            [
                a odrl:Constraint ;
                dcterms:description "Upon reasonable notice" ;
                odrl:leftOperand odrl:delayPeriod ;
                odrl:operator odrl:gteq ;
                odrl:rightOperand "P7D"^^xsd:duration
            ]
        )
    ] ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Elsevier must provide reasonable notice to the Developer before blocking, changing, suspending, removing or disabling access to the APIs and services." ;
        odrl:action odrl:inform ;
        odrl:target :apis ;
        odrl:assignee :Elsevier ;
        odrl:informedParty :Developer
    ] .