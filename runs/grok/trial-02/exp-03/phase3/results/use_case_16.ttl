@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    foaf:name "ELSEVIER" .

:developer a tosl:BusinessCustomer ;
    dcterms:description "The Developer" .

:application a odrl:Asset ;
    dcterms:description "The Application" .

:advertising a odrl:Asset ;
    dcterms:description "Any advertising" .

:apiContent a odrl:Asset ;
    dcterms:description "Content obtained via the API" .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.4: The Developer shall not display any advertising inside the Application or use any content as obtained via the API outside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibitionAd, :prohibitionContent ;
    odrl:assigner :Elsevier .

:prohibitionAd a odrl:Prohibition ;
    dcterms:description "The Developer shall not display any advertising inside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:action odrl:include ;
    odrl:target :advertising ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:virtualLocation ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :application
    ] .

:prohibitionContent a odrl:Prohibition ;
    dcterms:description "The Developer shall not use any content as obtained via the API outside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:virtualLocation ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :application
    ] .