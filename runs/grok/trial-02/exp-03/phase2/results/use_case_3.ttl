@prefix : <http://example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Elsevier a tosl:Provider ;
    foaf:name "Elsevier" .

:customer a tosl:Customer .

:agreement a odrl:Asset .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier reserves the right, at its sole discretion, to change the terms of this Agreement at any time with reasonable advance notice given to You."^^xsd:string ;
    odrl:action odrl:modify ;
    odrl:target :agreement ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:delayPeriod ;
        odrl:operator odrl:gteq ;
        odrl:rightOperand "P30D"^^xsd:duration
    ] ;
    odrl:duty [
        a odrl:Duty ;
        odrl:action odrl:inform ;
        odrl:target :agreement ;
        odrl:assignee :Elsevier ;
        odrl:informedParty :customer
    ] .