@base <http://example.com> .

:apiService a tosl:Service ;
    dcterms:description "Elsevier API service provided to customers." .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiService .

:liability01 a tosl:Liability ;
    dcterms:description "API service is provided on an 'as is' and 'as available' basis without warranties of any kind."^^xsd:string ;
    tosl:liableParty :ELSEVIER ;
    tosl:targetParty :customer ;
    rdf:value tosl:discontinuity .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier API service is provided on an 'as is' and 'as available' basis without warranties of any kind."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:assigner :ELSEVIER ;
    tosl:limitationOfLiability :liability01 .