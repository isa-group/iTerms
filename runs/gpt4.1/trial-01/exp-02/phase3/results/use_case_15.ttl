@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :agreement01 .

:customer a tosl:Customer ;
    dcterms:description "This party represents the developer or customer using Elsevier's API services." .

:trademarksAndLogos a odrl:Asset ;
    dcterms:description "Elsevier's trademarks, logos, and other brand features." .

:pressReleases a odrl:Asset ;
    dcterms:description "Press releases regarding the API Service Agreement or the relationship of the parties." .

:obligation01 a odrl:Duty ;
    dcterms:description "Developer must obtain written permission in advance from Elsevier to use Elsevier's trademarks, logos, or other brand features for promoting the Developer's Application." ;
    odrl:action odrl:obtainConsent ;
    odrl:target :trademarksAndLogos ;
    odrl:assignee :customer .

:obligation02 a odrl:Duty ;
    dcterms:description "Any press releases regarding this Agreement or the relationship of the parties require the prior approval of the parties." ;
    odrl:action odrl:obtainConsent ;
    odrl:target :pressReleases ;
    odrl:assignee :customer .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This API Service Agreement governs the use of Elsevier's API services, including requirements for use of trademarks, logos, and press releases."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligation01, :obligation02 ;
    odrl:assigner :Elsevier .