@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiService .

:apiService a tosl:Service ;
    dcterms:description "Elsevier API Service, as described in the API Service Agreement." .

:apiWebsite a odrl:Asset ;
    dcterms:description "The official website of the Elsevier API Service." .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier will post a notification on the API Service's website describing the modifications made." ;
    odrl:action odrl:inform ;
    odrl:target :apiWebsite ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "The notification must describe the modifications made to the API Service." ;
        odrl:leftOperand tosl:justification ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "modifications made"
    ] .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This API Service Agreement governs the use of Elsevier's API Service."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .