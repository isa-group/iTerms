@base <http://example.com> .

:apiService a tosl:Service ;
    dcterms:description "The Elsevier API service provided to users under the API Service Agreement." .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "This party represents the customer in the context of the Elsevier API Service Agreement." .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This API Service Agreement governs the use of Elsevier's API services."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:assigner :Elsevier ;
    tosl:liability :liability01 .

:liability01 a tosl:Liability ;
    dcterms:description "The API service is provided on an 'as is' and 'as available' basis without warranties of any kind."^^xsd:string ;
    tosl:liableParty :Elsevier ;
    tosl:targetParty :customer ;
    rdf:value tosl:anyLiability .