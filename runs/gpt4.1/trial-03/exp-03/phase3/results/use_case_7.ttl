@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiService .

:application a tosl:Customer ;
    dcterms:description "The client application accessing Elsevier's API Service." .

:apiService a tosl:Service ;
    dcterms:description "Elsevier's API Service as described in the API Service Agreement." .

:apiContentUsagePolicies a odrl:Asset ;
    dcterms:description "The API's Content Usage Policies at http://dev.elsevier.com/policy.html" ;
    foaf:homepage <http://dev.elsevier.com/policy.html> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier API Service Agreement governing access and use of Elsevier's API Service."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    dcterms:requires <http://dev.elsevier.com/policy.html> ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier may immediately suspend the Application's access to the API Service if, in Elsevier's sole judgment, the Application's usage negatively affects the availability or performance of the API Service, or the Application is in breach of the API's Content Usage Policies." ;
    odrl:action tosl:terminate ;
    odrl:target :apiService ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:or (
            [
                a odrl:Constraint ;
                dcterms:description "Application's usage negatively affects the availability or performance of the API Service." ;
                odrl:leftOperand tosl:effect ;
                odrl:operator odrl:isAnyOf ;
                odrl:rightOperand ( "negativeAvailability" "negativePerformance" )
            ]
            [
                a odrl:Constraint ;
                dcterms:description "Application is in breach of the API's Content Usage Policies." ;
                odrl:leftOperand tosl:compliance ;
                odrl:operator odrl:neq ;
                odrl:rightOperand :apiContentUsagePolicies
            ]
        )
    ] .