@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiService .

:application a tosl:Customer ;
    dcterms:description "The client application integrating with Elsevier APIs." .

:apiKey a odrl:Asset ;
    dcterms:description "The API Key issued by Elsevier for the specific Application under this Agreement." .

:developer a tosl:Customer ;
    dcterms:description "The developer or entity integrating the Application with Elsevier APIs." .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This Agreement governs the use of Elsevier APIs and associated API Keys by developers and their applications."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:prohibition :prohibition01 ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "The Application may use only the API Key issued by Elsevier upon execution of this Agreement." ;
    odrl:action odrl:use ;
    odrl:target :apiKey ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "API Key must be the one issued by Elsevier for this Application under this Agreement." ;
        odrl:leftOperand odrl:purpose ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :application
    ] .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application must not use any API Key issued for use by other applications and/or to other developers." ;
    odrl:action odrl:use ;
    odrl:target :apiKey ;
    odrl:assignee :application ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:or (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:purpose ;
                odrl:operator odrl:neq ;
                odrl:rightOperand :application
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:recipient ;
                odrl:operator odrl:neq ;
                odrl:rightOperand :developer
            ]
        )
    ] .