@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :apiService .

:developer a tosl:Customer ;
    dcterms:description "The Developer is the party integrating or using the Elsevier API in their Application." .

:advertising a odrl:Asset ;
    dcterms:description "Advertising content displayed inside the Application." .

:application a odrl:Asset ;
    dcterms:description "The Developer's Application that integrates the Elsevier API." .

:apiContent a odrl:Asset ;
    dcterms:description "Content as obtained via the Elsevier API." .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This Agreement governs the use of Elsevier's API and related content and services."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :Elsevier .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Developer shall not display any advertising inside the Application or use any content as obtained via the API outside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:assignee :developer ;
    odrl:action odrl:display ;
    odrl:target :advertising ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:spatial ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :application
    ] ;
    odrl:remedy [
        a odrl:Duty ;
        dcterms:description "Permission may be granted by Elsevier in writing."^^xsd:string ;
        odrl:action odrl:obtainConsent ;
        odrl:assignee :developer ;
        odrl:target :advertising
    ] .