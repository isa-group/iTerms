@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> ;
    odrl:assignerOf :elsevierContent .

:customer a tosl:Customer ;
    dcterms:description "This party represents the developer or customer in the context of the Elsevier API Service Agreement." .

:elsevierContent a odrl:Asset ;
    dcterms:description "Elsevier content and any derivative works based upon the Elsevier content." .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The unauthorized redistribution of the Elsevier content, including any derivative works, is not permitted. All right, title and interest in and to the Elsevier content and any derivative works remain with Elsevier and its suppliers, except as expressly set forth in this Agreement."^^xsd:string ;
    odrl:action odrl:distribute ;
    odrl:target :elsevierContent ;
    odrl:assignee :customer .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This API Service Agreement governs the use of Elsevier content and services by developers and customers."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :Elsevier .