@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API services referenced in this agreement."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:developer a tosl:BusinessCustomer ;
    dcterms:description "The Developer using Elsevier APIs under this agreement."^^xsd:string .

:pressReleases a tosl:UserContent ;
    dcterms:description "Press releases regarding this Agreement or the relationship of the parties."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Any press releases regarding this Agreement or the relationship of the parties require the prior approval of both Elsevier and the Developer."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation_press_dev, :obligation_press_elsevier ;
    odrl:assigner :Elsevier .

:obligation_press_dev a odrl:Duty ;
    dcterms:description "Developer must obtain prior written approval from Elsevier before issuing any press release regarding this Agreement or the relationship between the parties."^^xsd:string ;
    odrl:action odrl:obtainConsent ;
    odrl:target :pressReleases ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:recipient ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :Elsevier
    ] .

:obligation_press_elsevier a odrl:Duty ;
    dcterms:description "Elsevier must obtain prior written approval from the Developer before issuing any press release regarding this Agreement or the relationship between the parties."^^xsd:string ;
    odrl:action odrl:obtainConsent ;
    odrl:target :pressReleases ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:recipient ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :developer
    ] .