@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This Agreement is governed by the laws of The Netherlands and the parties irrevocably consent to the exclusive jurisdiction of the courts of The Netherlands for any action or suit arising out of or pertaining to this Agreement, except where local law requires otherwise."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01, :obligation02 ;
    tosl:onDispute :litigationNL_customer, :litigationNL_business ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is the assigner and provider for this API Service Agreement."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "Represents the consumer user of the Elsevier API services."^^xsd:string .

:businessCustomer a tosl:BusinessCustomer ;
    dcterms:description "Represents the business user of the Elsevier API services."^^xsd:string .

:litigationNL_customer a tosl:Litigation ;
    dcterms:description "Disputes will be governed by and construed in accordance with the laws of The Netherlands, with exclusive jurisdiction of the courts of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    tosl:targetParty :customer ;
    tosl:takesPlaceIn :netherlandsCourts ;
    tosl:governedBy :netherlandsLaw ;
    tosl:condition [
        a odrl:Constraint ;
        odrl:leftOperand :localLawRequiresDifferentJurisdiction ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .

:litigationNL_business a tosl:Litigation ;
    dcterms:description "Disputes will be governed by and construed in accordance with the laws of The Netherlands, with exclusive jurisdiction of the courts of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    tosl:targetParty :businessCustomer ;
    tosl:takesPlaceIn :netherlandsCourts ;
    tosl:governedBy :netherlandsLaw ;
    tosl:condition [
        a odrl:Constraint ;
        odrl:leftOperand :localLawRequiresDifferentJurisdiction ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .

:netherlandsCourts a tosl:Jurisdiction .
:netherlandsLaw a tosl:Law .

:obligation01 a odrl:Duty ;
    dcterms:description "The Customer irrevocably consents to the exclusive jurisdiction of the courts of The Netherlands for disputes arising out of or pertaining to this Agreement, except where local law requires otherwise."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand :localLawRequiresDifferentJurisdiction ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .

:obligation02 a odrl:Duty ;
    dcterms:description "The Business Customer irrevocably consents to the exclusive jurisdiction of the courts of The Netherlands for disputes arising out of or pertaining to this Agreement, except where local law requires otherwise."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :businessCustomer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand :localLawRequiresDifferentJurisdiction ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .

:localLawRequiresDifferentJurisdiction a odrl:LeftOperand ;
    rdfs:label "Local law requires a different jurisdiction"@en .