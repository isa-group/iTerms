@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Each party disclaims liability to the other except as expressly stated; no liability for special, indirect, or consequential damages, including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01 ;
    tosl:liability :liability01, :liability02 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., provider of the API Services."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "API consumer party to the Elsevier API Service Agreement."^^xsd:string .

:damages a odrl:Asset ;
    dcterms:description "Claims for special, indirect, or consequential damages, including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The customer may not claim special, indirect, or consequential damages (including loss of profits, loss of business, or unfitness for user purposes) from Elsevier under this Agreement."^^xsd:string ;
    odrl:action tosl:claim ;
    odrl:target :damages ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:effect ;
        odrl:operator odrl:isAnyOf ;
        odrl:rightOperand ( tosl:anyIndirectDamage tosl:anyLoss )
    ] .

:liability01 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, Elsevier disclaims all liability to the customer and is not liable for special, indirect, or consequential damages including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :ELSEVIER ;
    tosl:targetParty :customer ;
    rdf:value tosl:anyIndirectDamage, tosl:anyLoss .

:liability02 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, the customer disclaims all liability to Elsevier and is not liable for special, indirect, or consequential damages including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :customer ;
    tosl:targetParty :ELSEVIER ;
    rdf:value tosl:anyIndirectDamage, tosl:anyLoss .