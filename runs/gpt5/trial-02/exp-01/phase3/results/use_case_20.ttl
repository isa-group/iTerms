@base <http://example.com> .

:agreement a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier API Service Agreement dispute resolution and governing law terms."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01, :obligation02 ;
    tosl:onDispute :litigation01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., the provider of the API Services."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "API customer or subscriber."^^xsd:string .

:litigation01 a tosl:Litigation ;
    dcterms:description "This Agreement is governed by the laws of The Netherlands, and disputes are subject to the exclusive jurisdiction of the courts of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    tosl:targetParty :customer ;
    tosl:takesPlaceIn :netherlandsCourts ;
    tosl:governedBy :netherlandsLaw .

:netherlandsCourts a tosl:Jurisdiction .
:netherlandsLaw a tosl:Law .

:obligation01 a odrl:Duty ;
    dcterms:description "Customer irrevocably consents to the exclusive jurisdiction of the courts of The Netherlands for disputes arising out of or relating to this Agreement, except where local law requires otherwise."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement ;
    odrl:assignee :customer .

:obligation02 a odrl:Duty ;
    dcterms:description "Elsevier irrevocably consents to the exclusive jurisdiction of the courts of The Netherlands for disputes arising out of or relating to this Agreement, except where local law requires otherwise."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement ;
    odrl:assignee :ELSEVIER .