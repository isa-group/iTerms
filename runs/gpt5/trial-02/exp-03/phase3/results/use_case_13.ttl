@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.1.4: Developer acknowledges Elsevier and its suppliers retain all rights in the Elsevier content and any derivative works; unauthorized redistribution of Elsevier content is not permitted."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01, :prohibition02, :prohibition03 ;
    odrl:assigner :Elsevier .

:Elsevier a tosl:Provider ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://dev.elsevier.com/> .

:ElsevierSuppliers a tosl:Provider ;
    dcterms:description "Elsevier suppliers and licensors."^^xsd:string .

:developer a tosl:BusinessCustomer ;
    dcterms:description "Developer using the Elsevier APIs under this Agreement."^^xsd:string .

:elsevierContent a odrl:Asset ;
    dcterms:description "Elsevier content made available via the APIs."^^xsd:string ;
    dcterms:rightsHolder :Elsevier, :ElsevierSuppliers .

:derivativeWorks a odrl:Asset ;
    dcterms:description "Derivative works based upon Elsevier content."^^xsd:string ;
    dcterms:rightsHolder :Elsevier, :ElsevierSuppliers .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "Unauthorized redistribution of Elsevier content is not permitted."^^xsd:string ;
    odrl:action odrl:distribute ;
    odrl:target :elsevierContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Prohibition applies when redistribution is not expressly authorized under this Agreement."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :agreement01
    ] .

:prohibition02 a odrl:Prohibition ;
    dcterms:description "Developer must not claim ownership or exclusive rights in Elsevier content; all right, title and interest remain with Elsevier and its suppliers."^^xsd:string ;
    odrl:action tosl:claim ;
    odrl:target :elsevierContent ;
    odrl:assignee :developer .

:prohibition03 a odrl:Prohibition ;
    dcterms:description "Developer must not claim ownership or exclusive rights in derivative works based upon Elsevier content."^^xsd:string ;
    odrl:action tosl:claim ;
    odrl:target :derivativeWorks ;
    odrl:assignee :developer .