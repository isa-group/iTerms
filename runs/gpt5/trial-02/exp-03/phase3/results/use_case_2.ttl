@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "This party represents the user of Elsevier's APIs or services."^^xsd:string .

:institutionAgreement a odrl:Asset ;
    dcterms:description "The separate agreement between Elsevier and the user's institution, company, or organization."^^xsd:string .

:useCaseAgreement a odrl:Asset ;
    dcterms:description "The specific use-case agreement between Elsevier and the user when no institutional agreement applies."^^xsd:string .

:hasInstitutionAgreement a odrl:LeftOperand .

:obligation01 a odrl:Duty ;
    dcterms:description "You accept and agree to the terms and conditions of this Agreement."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :customer .

:obligation02 a odrl:Duty ;
    dcterms:description "You accept and agree to the terms and conditions of the agreement between Elsevier and your institution, company, or organization (if applicable)."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :institutionAgreement ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand :hasInstitutionAgreement ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "true"^^xsd:boolean
    ] .

:obligation03 a odrl:Duty ;
    dcterms:description "If no institutional agreement applies, you accept and agree to the specific use-case agreement between Elsevier and you."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :useCaseAgreement ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand :hasInstitutionAgreement ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "You accept and agree to this Agreement and, if applicable, to the agreement between Elsevier and your institution; otherwise, to the specific use-case agreement between Elsevier and you."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01, :obligation02, :obligation03 ;
    odrl:assigner :ELSEVIER .