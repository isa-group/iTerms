@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 2.2: Upon breach, Elsevier may block, change, suspend, remove, or disable access to the APIs and services with reasonable notice to Developer."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :OpenAI ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:assigner :OpenAI .

:OpenAI a tosl:Provider ;
    dcterms:description "Elsevier provides APIs and services under the API SERVICE AGREEMENT."^^xsd:string ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://dev.elsevier.com/> .

:developer a tosl:BusinessCustomer ;
    dcterms:description "Developer using Elsevier APIs under this Agreement."^^xsd:string .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs and related services."^^xsd:string .

:permission01 a odrl:Permission ;
    dcterms:description "In the event of a breach of this Agreement, Elsevier may block, change, suspend, remove, or disable access to the APIs and services at any time upon reasonable notice to Developer."^^xsd:string ;
    odrl:action odrl:modify, tosl:terminate, tosl:remove ;
    odrl:target :apis ;
    odrl:assignee :OpenAI ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Applies when there is a breach of the terms and conditions of this Agreement."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :agreement01
    ] ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide reasonable notice to Developer before taking enforcement action on access."^^xsd:string ;
        odrl:action odrl:inform ;
        odrl:target :apis ;
        odrl:assignee :OpenAI ;
        odrl:informedParty :developer ;
        odrl:constraint [
            a odrl:Constraint ;
            dcterms:description "Notice must be reasonable."^^xsd:string ;
            odrl:leftOperand tosl:justification ;
            odrl:operator odrl:eq ;
            odrl:rightOperand "reasonable notice"^^xsd:string
        ]
    ] .