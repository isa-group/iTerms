@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:apiService a tosl:Service ;
    dcterms:description "Elsevier API Service."^^xsd:string .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API Service."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier may immediately suspend an Application’s access to the API Service if its use degrades availability or performance, or if it breaches the API Content Usage Policies."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    dcterms:requires <http://dev.elsevier.com/policy.html> ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier (provider) may immediately terminate/suspend the Application’s access to the API Service if usage degrades availability or performance, or if the Application is not compliant with the API Content Usage Policies."^^xsd:string ;
    odrl:action tosl:terminate ;
    odrl:target :apiService ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Suspension takes effect immediately."^^xsd:string ;
        odrl:leftOperand odrl:delayPeriod ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "PT0S"^^xsd:duration
    ] ,
    [
        a odrl:LogicalConstraint ;
        odrl:or (
            [
                a odrl:Constraint ;
                dcterms:description "Usage negatively affects availability or performance."^^xsd:string ;
                odrl:leftOperand tosl:effect ;
                odrl:operator odrl:eq ;
                odrl:rightOperand tosl:degradation
            ]
            [
                a odrl:Constraint ;
                dcterms:description "Breach of API Content Usage Policies."^^xsd:string ;
                odrl:leftOperand tosl:compliance ;
                odrl:operator odrl:neq ;
                odrl:rightOperand <http://dev.elsevier.com/policy.html>
            ]
        )
    ] .