@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "This party represents the customer using Elsevier APIs under the API Service Agreement."^^xsd:string .

:institutionAgreement a odrl:Asset ;
    dcterms:title "Institution/Company/Organization Agreement or Specific Use-Case Agreement"^^xsd:string ;
    dcterms:description "The agreement between Elsevier and the user's institution, company, or organization; if not applicable, the agreement between Elsevier and the user for the specific use case."^^xsd:string .

:agreements a odrl:AssetCollection ;
    dcterms:description "Collection including this API Service Agreement and the applicable institutional or specific use-case agreement."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier API Service Agreement governing the use of Elsevier APIs."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01 ;
    odrl:assigner :Elsevier .

:agreement01 odrl:partOf :agreements .
:institutionAgreement odrl:partOf :agreements .

:obligation01 a odrl:Duty ;
    dcterms:description "You accept and agree to the terms and conditions of this Agreement and the terms and conditions of the agreement between Elsevier and your institution, company or organization or, if not applicable, as agreed between Elsevier and you for your specific use case."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:consentType ;
        odrl:operator odrl:eq ;
        odrl:rightOperand tosl:explicitConsent
    ] ;
    odrl:target :agreements ;
    odrl:assignee :customer .