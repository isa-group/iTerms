@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V. is the provider of the API Services under this Agreement."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "This party represents the API customer under the Elsevier API Service Agreement."^^xsd:string .

:apiServices a tosl:Service ;
    dcterms:description "Elsevier API Services made available to the customer under this Agreement."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Liability disclaimer regarding indirect or consequential damages between the parties."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    tosl:limitationOfLiability :liability01, :liability02 ;
    odrl:assigner :ELSEVIER .

:permission01 a odrl:Permission ;
    dcterms:description "Customer may use the Elsevier API Services subject to this Agreement."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiServices ;
    odrl:assignee :customer ;
    odrl:assigner :ELSEVIER .

:liability01 a tosl:Liability ;
    dcterms:description "Except as expressly stated in the Agreement, Elsevier disclaims liability to the customer in connection with performance of the Agreement; in no event is Elsevier liable for special, indirect, or consequential damages, including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :ELSEVIER ;
    tosl:targetParty :customer ;
    rdf:value tosl:anyIndirectDamage, tosl:anyLoss ;
    tosl:limitation [
        a odrl:Constraint ;
        odrl:leftOperand tosl:effect ;
        odrl:operator odrl:eq ;
        odrl:rightOperand tosl:breachOfContract
    ] .

:liability02 a tosl:Liability ;
    dcterms:description "Except as expressly stated in the Agreement, the customer disclaims liability to Elsevier in connection with performance of the Agreement; in no event is the customer liable for special, indirect, or consequential damages, including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :customer ;
    tosl:targetParty :ELSEVIER ;
    rdf:value tosl:anyIndirectDamage, tosl:anyLoss ;
    tosl:limitation [
        a odrl:Constraint ;
        odrl:leftOperand tosl:effect ;
        odrl:operator odrl:eq ;
        odrl:rightOperand tosl:breachOfContract
    ] .