@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is the assigner/provider for the API services."^^xsd:string ;
    foaf:name "ELSEVIER" .

:customer a tosl:Customer ;
    dcterms:description "This party represents the developer using Elsevier's API."^^xsd:string .

:apiContent a odrl:Asset ;
    dcterms:description "Content obtained via the Elsevier API."^^xsd:string .

:useLocation a odrl:LeftOperand .
:outsideApplication a odrl:RightOperand .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.4 prohibits using content obtained via the API outside the Application unless Elsevier provides written permission."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :ELSEVIER .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "You must not use content obtained via the API outside the Application, unless Elsevier has provided written permission."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiContent ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand :useLocation ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :outsideApplication
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand tosl:consentType ;
                odrl:operator odrl:neq ;
                odrl:rightOperand tosl:writtenPermission
            ]
        )
    ] .