@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    foaf:name "ELSEVIER"^^xsd:string ;
    dcterms:description "Elsevier is the provider and issuer of API Keys under the API Service Agreement."^^xsd:string .

:customer a tosl:Customer ;
    dcterms:description "The developer/customer using the Application and Elsevier APIs."^^xsd:string .

:application a odrl:Asset ;
    dcterms:description "The Customer’s Application that integrates Elsevier APIs."^^xsd:string .

:apiKey a odrl:Asset ;
    dcterms:description "API Key issued by Elsevier for the Customer’s Application upon execution of the Agreement."^^xsd:string .

:issuer a odrl:LeftOperand .
:issuedFor a odrl:LeftOperand .
:issuedTo a odrl:LeftOperand .

:obligation01 a odrl:Duty ;
    dcterms:description "The Application will use the API Key issued by Elsevier upon execution of this Agreement, and no other API Key issued for use by other applications and/or to other developers."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiKey ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand :issuer ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :Elsevier
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand :issuedFor ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :application
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand :issuedTo ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :customer
            ]
        )
    ] .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligation01 ;
    odrl:assigner :Elsevier .