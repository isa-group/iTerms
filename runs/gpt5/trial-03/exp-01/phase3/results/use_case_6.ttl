@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., provider of the APIs and related materials."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> ;
    odrl:assignerOf :agreement01 .

:customer a tosl:Customer ;
    dcterms:description "The developer or organization using Elsevier APIs under this agreement."^^xsd:string .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs and related materials."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing access to and use of Elsevier APIs and related materials."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    dcterms:requires <https://dev.elsevier.com/policy_and_guidelines/> ;
    odrl:permission :permission01 ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier grants you a limited license to use its APIs and related materials to develop and integrate your application, subject to usage policies."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apis ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Subject to compliance with Elsevier's usage policies available on the developer portal."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand <https://dev.elsevier.com/policy_and_guidelines/>
    ] .

:obligation01 a odrl:Duty ;
    dcterms:description "All notices will be delivered in writing to the email listed in the developer portal."^^xsd:string ;
    odrl:action odrl:inform ;
    odrl:target :agreement01 ;
    odrl:assignee :ELSEVIER ;
    odrl:informedParty :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:deliveryChannel ;
        odrl:operator odrl:eq ;
        odrl:rightOperand <mailto:email-listed-in-developer-portal>
    ] .