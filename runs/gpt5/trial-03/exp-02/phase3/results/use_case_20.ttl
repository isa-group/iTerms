@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier B.V., provider of the Elsevier APIs and related services."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> ;
    odrl:assignerOf :agreement01 .

:customer a tosl:Customer ;
    dcterms:description "This party represents an individual customer using Elsevier APIs."^^xsd:string .

:businessCustomer a tosl:BusinessCustomer ;
    dcterms:description "This party represents a business customer using Elsevier APIs."^^xsd:string .

:netherlandsCourts a tosl:Jurisdiction .
:netherlandsLaw a tosl:Law .

:localLawRequiresOtherwise a odrl:LeftOperand .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This Agreement is governed by the laws of The Netherlands and the parties irrevocably consent to the exclusive jurisdiction of the courts of The Netherlands for disputes arising out of or related to this Agreement, except where local law requires otherwise."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    tosl:onDispute :litigationCustomer, :litigationBusiness ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "Permission for the customer to read the Agreement."^^xsd:string ;
    odrl:action odrl:read ;
    odrl:target :agreement01 ;
    odrl:assignee :customer .

:litigationCustomer a tosl:Litigation ;
    dcterms:description "Exclusive jurisdiction of the courts of The Netherlands; governed by the laws of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    tosl:targetParty :customer ;
    tosl:takesPlaceIn :netherlandsCourts ;
    tosl:governedBy :netherlandsLaw ;
    tosl:condition [
        a odrl:Constraint ;
        odrl:leftOperand :localLawRequiresOtherwise ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .

:litigationBusiness a tosl:Litigation ;
    dcterms:description "Exclusive jurisdiction of the courts of The Netherlands; governed by the laws of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    tosl:targetParty :businessCustomer ;
    tosl:takesPlaceIn :netherlandsCourts ;
    tosl:governedBy :netherlandsLaw ;
    tosl:condition [
        a odrl:Constraint ;
        odrl:leftOperand :localLawRequiresOtherwise ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .