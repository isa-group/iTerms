@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:apiContent a odrl:Asset ;
    dcterms:description "Content obtained via Elsevier API."^^xsd:string .

:application a tosl:Service ;
    dcterms:description "The Developer's Application that integrates Elsevier API."^^xsd:string .

:customer a tosl:Customer ;
    dcterms:description "This party represents the developer using the Elsevier API."^^xsd:string .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the assigner and API provider."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:prohibitionAds a odrl:Prohibition ;
    dcterms:description "The Developer shall not display any advertising inside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:action odrl:display ;
    odrl:target :application ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:purpose ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :advertising
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand tosl:licensingType ;
                odrl:operator odrl:neq ;
                odrl:rightOperand :writtenPermission
            ]
        )
    ] .

:prohibitionOutside a odrl:Prohibition ;
    dcterms:description "The Developer shall not use any content obtained via the API outside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiContent ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:purpose ;
                odrl:operator odrl:neq ;
                odrl:rightOperand :applicationUse
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand tosl:licensingType ;
                odrl:operator odrl:neq ;
                odrl:rightOperand :writtenPermission
            ]
        )
    ] .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.4: The Developer shall not display any advertising inside the Application or use any content as obtained via the API outside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibitionAds, :prohibitionOutside ;
    odrl:assigner :Elsevier .