@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API Service."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:apiAccess a tosl:Service ;
    dcterms:description "Access to Elsevier's API Service."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier may immediately suspend the Application's access to the API Service if, in Elsevier's sole judgment, usage negatively affects availability or performance, or the Application is in breach of the API Content Usage Policies."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "Right to immediately suspend the Application's access to the API Service if, in Elsevier's sole judgment, there is negative impact on availability/performance or a breach of the API Content Usage Policies."^^xsd:string ;
    odrl:action tosl:terminate ;
    odrl:target :apiAccess ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                dcterms:description "Decision made at Elsevier's sole judgment."^^xsd:string ;
                odrl:leftOperand tosl:justification ;
                odrl:operator odrl:eq ;
                odrl:rightOperand "providerSoleJudgment"^^xsd:string
            ]
            [
                a odrl:LogicalConstraint ;
                odrl:or (
                    [
                        a odrl:Constraint ;
                        dcterms:description "Application usage negatively affects availability or performance of the API Service."^^xsd:string ;
                        odrl:leftOperand tosl:effect ;
                        odrl:operator odrl:eq ;
                        odrl:rightOperand :negativeAvailabilityOrPerformance
                    ]
                    [
                        a odrl:Constraint ;
                        dcterms:description "Application is in breach of the API Content Usage Policies."^^xsd:string ;
                        odrl:leftOperand tosl:compliance ;
                        odrl:operator odrl:neq ;
                        odrl:rightOperand <http://dev.elsevier.com/policy.html>
                    ]
                )
            ]
        )
    ] ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Suspension may be immediate (no delay)."^^xsd:string ;
        odrl:leftOperand odrl:delayPeriod ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "P0D"^^xsd:duration
    ] .