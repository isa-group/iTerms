@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics company."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> ;
    odrl:assignerOf :agreement01 .

:customer a tosl:Customer ;
    dcterms:description "This party represents the developer/customer using Elsevier APIs."^^xsd:string .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs and related materials."^^xsd:string .

:usagePolicies a odrl:Asset ;
    dcterms:description "Usage policies applicable to Elsevier APIs."^^xsd:string .

:developerPortalEmail a odrl:Asset ;
    dcterms:description "The email address listed in the developer portal for notices."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement granting a limited license to use Elsevier APIs and related materials, subject to usage policies and notice requirements."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    dcterms:requires :usagePolicies ;
    odrl:permission :permission01 ;
    odrl:assigner :ELSEVIER .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier grants you a limited license to use its APIs and related materials to develop and integrate your application, subject to usage policies, with all notices delivered in writing to the email listed in the developer portal."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apis ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Usage must comply with applicable usage policies."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :usagePolicies
    ], [
        a odrl:Constraint ;
        dcterms:description "Permitted purpose limited to developing and integrating your application."^^xsd:string ;
        odrl:leftOperand odrl:purpose ;
        odrl:operator odrl:isAnyOf ;
        odrl:rightOperand ( :developApp :integrateApp )
    ], [
        a odrl:Constraint ;
        dcterms:description "All notices are delivered in writing to the email listed in the developer portal."^^xsd:string ;
        odrl:leftOperand odrl:deliveryChannel ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :developerPortalEmail
    ] .