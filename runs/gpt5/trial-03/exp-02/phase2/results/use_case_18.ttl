:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API Services covered by this Agreement."^^xsd:string ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://dev.elsevier.com/> .

:businessCustomer a tosl:BusinessCustomer ;
    dcterms:description "This party represents the API customer using Elsevier's services."^^xsd:string .

:damages a odrl:Asset ;
    dcterms:description "Special, indirect, or consequential damages, including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Except as expressly stated in this Agreement, each party disclaims all liability to the other in connection with performance of this Agreement; and in no event will the infringing party be liable for special, indirect, or consequential damages including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01 ;
    tosl:limitationOfLiability :liability01, :liability02 ;
    odrl:assigner :ELSEVIER .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "Neither party may claim special, indirect, or consequential damages including loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    odrl:action tosl:claim ;
    odrl:target :damages ;
    odrl:assignee :businessCustomer, :ELSEVIER .

:liability01 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, Elsevier disclaims all liability to the business customer in connection with the parties' performance of this Agreement, and is not liable for special, indirect, or consequential damages (including loss of profits, loss of business, or unfitness for user purposes), including when alleged due to infringement or breach."^^xsd:string ;
    tosl:liableParty :ELSEVIER ;
    tosl:targetParty :businessCustomer ;
    rdf:value tosl:anyLiability, tosl:anyIndirectDamage, tosl:anyLoss ;
    tosl:limitation [
        a odrl:LogicalConstraint ;
        odrl:or (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:purpose ;
                odrl:operator odrl:isAnyOf ;
                odrl:rightOperand ( :performanceOfAgreement )
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand tosl:effect ;
                odrl:operator odrl:isAnyOf ;
                odrl:rightOperand ( tosl:breachOfContract )
            ]
        )
    ] .

:liability02 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, the business customer disclaims all liability to Elsevier in connection with the parties' performance of this Agreement, and is not liable for special, indirect, or consequential damages (including loss of profits, loss of business, or unfitness for user purposes), including when alleged due to infringement or breach."^^xsd:string ;
    tosl:liableParty :businessCustomer ;
    tosl:targetParty :ELSEVIER ;
    rdf:value tosl:anyLiability, tosl:anyIndirectDamage, tosl:anyLoss ;
    tosl:limitation [
        a odrl:LogicalConstraint ;
        odrl:or (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:purpose ;
                odrl:operator odrl:isAnyOf ;
                odrl:rightOperand ( :performanceOfAgreement )
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand tosl:effect ;
                odrl:operator odrl:isAnyOf ;
                odrl:rightOperand ( tosl:breachOfContract )
            ]
        )
    ] .