@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:apisAndServices a tosl:Service ;
    dcterms:description "Elsevier APIs and services."^^xsd:string .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., provider of APIs and services."^^xsd:string ;
    foaf:name "Elsevier" .

:developer a tosl:BusinessCustomer ;
    dcterms:description "Developer using Elsevier APIs and services."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 2.2 reserves Elsevier's right to block, change, suspend, remove or disable access to the APIs and services upon reasonable notice."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:assigner :ELSEVIER .

:permission01 a odrl:Permission ;
    dcterms:description "In case of breach of this Agreement, Elsevier may block, change, suspend, remove, or disable access to the APIs and any of its services at any time upon reasonable notice to Developer."^^xsd:string ;
    odrl:action tosl:remove ;
    odrl:target :apisAndServices ;
    odrl:assignee :ELSEVIER ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Applies when there is a breach of the terms and conditions of this Agreement."^^xsd:string ;
        odrl:leftOperand tosl:effect ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :breachOfAgreement
    ] ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide reasonable notice to Developer before blocking, changing, suspending, removing, or disabling access."^^xsd:string ;
        odrl:action odrl:inform ;
        odrl:target :apisAndServices ;
        odrl:assignee :ELSEVIER ;
        odrl:informedParty :developer
    ] .