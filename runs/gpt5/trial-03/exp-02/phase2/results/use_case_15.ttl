@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "Developer using the Elsevier APIs"^^xsd:string .

:elsevierTrademarks a odrl:Asset ;
    dcterms:description "Elsevier's trademarks, logos, and other brand features."^^xsd:string .

:pressReleases a tosl:UserContent ;
    dcterms:description "Press releases regarding this Agreement or the relationship of the parties."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.3 requires the Developer to obtain written, prior permission from Elsevier to use Elsevier's trademarks for promotion, and to secure prior approval from both parties for any press releases about the Agreement or their relationship."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01, :obligation02 ;
    odrl:assigner :Elsevier .

:obligation01 a odrl:Duty ;
    dcterms:description "Obtain written permission in advance from Elsevier before using Elsevier's trademarks, logos, or other brand features to promote the Developer's Application."^^xsd:string ;
    odrl:action odrl:obtainConsent ;
    odrl:target :elsevierTrademarks ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:recipient ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :Elsevier
    ] .

:obligation02 a odrl:Duty ;
    dcterms:description "Any press releases regarding this Agreement or the relationship of the parties require prior approval of both parties."^^xsd:string ;
    odrl:action odrl:obtainConsent ;
    odrl:target :pressReleases ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:recipient ;
        odrl:operator odrl:isAllOf ;
        odrl:rightOperand ( :Elsevier :customer )
    ] .