@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., the provider of Elsevier APIs and services."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:developer a tosl:BusinessCustomer ;
    dcterms:description "The Developer using Elsevier APIs under the API Service Agreement."^^xsd:string .

:application a tosl:Service ;
    dcterms:description "The Developer's Application that integrates with the Elsevier API."^^xsd:string .

:apiContent a odrl:Asset ;
    dcterms:description "Content obtained via the Elsevier API."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.4: The Developer shall not display advertising inside the Application or use content obtained via the API outside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01, :prohibition02 ;
    odrl:assigner :ELSEVIER .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Developer shall not display any advertising inside the Application unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:action odrl:display ;
    odrl:target :application ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:purpose ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :advertising
    ] ,
    [
        a odrl:Constraint ;
        odrl:leftOperand tosl:licensingType ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :writtenPermission
    ] .

:prohibition02 a odrl:Prohibition ;
    dcterms:description "The Developer shall not use any content obtained via the API outside the Application unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:virtualLocation ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :application
    ] ,
    [
        a odrl:Constraint ;
        odrl:leftOperand tosl:licensingType ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :writtenPermission
    ] .