@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "The term commences upon the Developer’s acceptance and continues until either party terminates the Agreement in writing or Elsevier deactivates the API Key."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permTerminateDev, :permTerminateElsevier, :permTerminateOnDeactivation ;
    odrl:obligation :obligationAccept ;
    odrl:assigner :Elsevier .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API services under this Agreement."^^xsd:string ;
    foaf:name "ELSEVIER" .

:developer a tosl:Customer ;
    dcterms:description "The developer party using Elsevier’s APIs under this Agreement."^^xsd:string .

:obligationAccept a odrl:Duty ;
    dcterms:description "The Developer accepts this Agreement, which marks the commencement of the term." ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :developer .

:permTerminateDev a odrl:Permission ;
    dcterms:description "The Developer may terminate this Agreement by providing written notice." ;
    odrl:action tosl:terminate ;
    odrl:target :agreement01 ;
    odrl:assignee :developer ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide written notice of termination." ;
        odrl:action odrl:inform ;
        odrl:target :agreement01 ;
        odrl:assignee :developer ;
        odrl:constraint [
            a odrl:Constraint ;
            odrl:leftOperand odrl:deliveryChannel ;
            odrl:operator odrl:eq ;
            odrl:rightOperand :writtenNotice
        ]
    ] .

:permTerminateElsevier a odrl:Permission ;
    dcterms:description "Elsevier may terminate this Agreement by providing written notice." ;
    odrl:action tosl:terminate ;
    odrl:target :agreement01 ;
    odrl:assignee :Elsevier ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide written notice of termination." ;
        odrl:action odrl:inform ;
        odrl:target :agreement01 ;
        odrl:assignee :Elsevier ;
        odrl:constraint [
            a odrl:Constraint ;
            odrl:leftOperand odrl:deliveryChannel ;
            odrl:operator odrl:eq ;
            odrl:rightOperand :writtenNotice
        ]
    ] .

:permTerminateOnDeactivation a odrl:Permission ;
    dcterms:description "Elsevier may terminate this Agreement upon deactivation of the API Key by Elsevier." ;
    odrl:action tosl:terminate ;
    odrl:target :agreement01 ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :apiKeyDeactivated
    ] .