@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier B.V., the provider of the API Service."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "API user or licensee."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Policy terms governing use of Elsevier APIs."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier may change the terms of this Agreement at its sole discretion, provided reasonable advance notice is given to the customer."^^xsd:string ;
    odrl:action odrl:modify ;
    odrl:target :agreement01 ;
    odrl:assignee :Elsevier ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide reasonable advance notice to the customer before changes take effect."^^xsd:string ;
        odrl:action odrl:inform ;
        odrl:target :agreement01 ;
        odrl:assignee :Elsevier ;
        odrl:informedParty :customer ;
        odrl:constraint [
            a odrl:Constraint ;
            dcterms:description "Notice must be given in advance (greater than zero days)."^^xsd:string ;
            odrl:leftOperand odrl:delayPeriod ;
            odrl:operator odrl:gt ;
            odrl:rightOperand "P0D"^^xsd:duration
        ] , [
            a odrl:Constraint ;
            dcterms:description "The notice must be reasonable."^^xsd:string ;
            odrl:leftOperand tosl:justification ;
            odrl:operator odrl:eq ;
            odrl:rightOperand "reasonable"^^xsd:string
        ]
    ] .