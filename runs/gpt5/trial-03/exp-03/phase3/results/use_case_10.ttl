@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier provides APIs and content services."^^xsd:string ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "API application developer using Elsevier APIs."^^xsd:string .

:elsevierContent a odrl:Asset ;
    dcterms:description "Elsevier content accessed via Elsevier APIs."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.1.1: the Application will not use Elsevier content other than allowed by the API's Content Usage Policies."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    dcterms:requires <http://dev.elsevier.com/policy.html> ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :Elsevier .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Application will not use Elsevier content except as allowed by the API’s Content Usage Policies."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :elsevierContent ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Prohibition applies when use is not compliant with the API’s Content Usage Policies."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:neq ;
        odrl:rightOperand <http://dev.elsevier.com/policy.html>
    ] .