@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause: Elsevier will also post a notification on the API Service's web site describing the modifications made."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API Service."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:apiService a tosl:Service ;
    dcterms:description "Elsevier API Service." .

:apiWebsite a tosl:Service ;
    dcterms:description "The API Service's web site." .

:modificationNotice a odrl:Asset ;
    dcterms:description "Notification describing the modifications made." .

:obligation01 a odrl:Duty ;
    dcterms:description "Elsevier will post a notification on the API Service's web site describing the modifications made."^^xsd:string ;
    odrl:action tosl:publish ;
    odrl:target :modificationNotice ;
    odrl:assignee :ELSEVIER ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "The notification must be posted on the API Service's website." ;
        odrl:leftOperand odrl:deliveryChannel ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :apiWebsite
    ] .