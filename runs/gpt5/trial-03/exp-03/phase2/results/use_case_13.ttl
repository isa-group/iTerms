@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Clause 3.1.4: Rights in Elsevier content and derivative works remain with Elsevier and its suppliers; unauthorized redistribution of Elsevier content is not permitted."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :Elsevier .

:Elsevier a tosl:Provider ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://www.elsevier.com/> ;
    dcterms:description "Elsevier is the provider of the Elsevier content and retains rights in the content and derivative works."^^xsd:string .

:elsevierSuppliers a tosl:Provider ;
    dcterms:description "Suppliers of Elsevier who may hold rights in Elsevier content and derivative works."^^xsd:string .

:developer a tosl:BusinessCustomer ;
    dcterms:description "The Developer party subject to the API Service Agreement."^^xsd:string .

:elsevierContent a odrl:Asset ;
    dcterms:description "Elsevier content made available via the APIs."^^xsd:string ;
    dcterms:rightsHolder :Elsevier, :elsevierSuppliers .

:derivativeWorks a odrl:Asset ;
    dcterms:description "Derivative works based upon the Elsevier content."^^xsd:string ;
    dcterms:source :elsevierContent ;
    dcterms:rightsHolder :Elsevier, :elsevierSuppliers .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "Unauthorized redistribution of Elsevier content is not permitted."^^xsd:string ;
    odrl:action odrl:distribute ;
    odrl:target :elsevierContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:licensingType ;
        odrl:operator odrl:neq ;
        odrl:rightOperand "authorized"^^xsd:string
    ] .