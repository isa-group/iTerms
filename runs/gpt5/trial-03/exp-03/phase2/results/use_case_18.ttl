@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Liability disclaimer and limitation for performance of the Agreement and for indirect, special, or consequential damages."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    tosl:limitationOfLiability :liability01, :liability02, :liability03, :liability04 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., provider of the Elsevier APIs under this Agreement."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "API consumer party under the Elsevier API Service Agreement."^^xsd:string .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs and related services."^^xsd:string .

:permission01 a odrl:Permission ;
    dcterms:description "Customer may use the Elsevier APIs subject to this Agreement."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apis ;
    odrl:assignee :customer .

:liability01 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, Elsevier disclaims all liability to the customer in connection with the parties’ performance of this Agreement."^^xsd:string ;
    tosl:liableParty :ELSEVIER ;
    tosl:targetParty :customer ;
    rdf:value tosl:anyLiability ;
    tosl:limitation [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:event ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :performanceOfAgreement
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand :expresslyStatedException ;
                odrl:operator odrl:eq ;
                odrl:rightOperand "false"^^xsd:boolean
            ]
        )
    ] .

:liability02 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, the customer disclaims all liability to Elsevier in connection with the parties’ performance of this Agreement."^^xsd:string ;
    tosl:liableParty :customer ;
    tosl:targetParty :ELSEVIER ;
    rdf:value tosl:anyLiability ;
    tosl:limitation [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:event ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :performanceOfAgreement
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand :expresslyStatedException ;
                odrl:operator odrl:eq ;
                odrl:rightOperand "false"^^xsd:boolean
            ]
        )
    ] .

:liability03 a tosl:Liability ;
    dcterms:description "In no event will Elsevier, when it is the infringing party, be liable to the customer for special, indirect, or consequential damages, including but not limited to loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :ELSEVIER ;
    tosl:targetParty :customer ;
    rdf:value tosl:anyIndirectDamage, tosl:anyLoss ;
    tosl:limitation [
        a odrl:Constraint ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :infringementOfAgreement
    ] .

:liability04 a tosl:Liability ;
    dcterms:description "In no event will the customer, when it is the infringing party, be liable to Elsevier for special, indirect, or consequential damages, including but not limited to loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :customer ;
    tosl:targetParty :ELSEVIER ;
    rdf:value tosl:anyIndirectDamage, tosl:anyLoss ;
    tosl:limitation [
        a odrl:Constraint ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :infringementOfAgreement
    ] .

:expresslyStatedException a odrl:LeftOperand .