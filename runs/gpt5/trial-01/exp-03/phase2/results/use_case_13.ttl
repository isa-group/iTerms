@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Developer acknowledges Elsevier retains all right, title, and interest in Elsevier Content and derivative works; unauthorized redistribution of Elsevier Content is not permitted."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01 ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V. is the provider of the APIs and Elsevier Content."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:developer a tosl:BusinessCustomer ;
    dcterms:description "The Developer using the Elsevier APIs under this Agreement."^^xsd:string .

:Supplier a tosl:Provider ;
    foaf:name "Elsevier suppliers" .

:elsevierContent a odrl:Asset ;
    dcterms:description "Elsevier Content accessible via the APIs."^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER, :Supplier .

:derivativeWorks a odrl:Asset ;
    dcterms:description "Derivative works based upon the Elsevier Content."^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER, :Supplier .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "Unauthorized redistribution of the Elsevier Content is not permitted."^^xsd:string ;
    odrl:action odrl:distribute ;
    odrl:target :elsevierContent ;
    odrl:assignee :developer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "This prohibition applies when the redistribution is not authorized under this Agreement."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:neq ;
        odrl:rightOperand :authorized
    ] .

:obligation01 a odrl:Duty ;
    dcterms:description "Developer acknowledges that all right, title, and interest in the Elsevier Content and any derivative works remain with Elsevier and its suppliers."^^xsd:string ;
    odrl:action odrl:attribute ;
    odrl:target :elsevierContent ;
    odrl:assignee :developer .