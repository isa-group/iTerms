@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "The term commences on the Developerâ€™s acceptance and continues until either party terminates in writing or Elsevier deactivates the API Key."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permTerminateByElsevier, :permTerminateByDeveloper, :permDeactivateAPIKey ;
    odrl:obligation :obligationAcceptance ;
    odrl:assigner :Elsevier .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier B.V., provider of the Elsevier APIs."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:developer a tosl:Customer ;
    dcterms:description "Developer using Elsevier APIs under this Agreement."^^xsd:string .

:apiKey a odrl:Asset ;
    dcterms:description "API Key issued by Elsevier for access to the APIs."^^xsd:string .

:obligationAcceptance a odrl:Duty ;
    dcterms:description "The Agreement commences when the Developer accepts this Agreement." ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :developer .

:permTerminateByElsevier a odrl:Permission ;
    dcterms:description "Elsevier may terminate the Agreement in writing." ;
    odrl:action tosl:terminate ;
    odrl:target :agreement01 ;
    odrl:assignee :Elsevier ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide written notice of termination to the Developer." ;
        odrl:action odrl:inform ;
        odrl:target :agreement01 ;
        odrl:assignee :Elsevier ;
        odrl:informedParty :developer ;
        odrl:constraint [
            a odrl:Constraint ;
            odrl:leftOperand odrl:deliveryChannel ;
            odrl:operator odrl:eq ;
            odrl:rightOperand "writing"^^xsd:string
        ]
    ] .

:permTerminateByDeveloper a odrl:Permission ;
    dcterms:description "The Developer may terminate the Agreement in writing." ;
    odrl:action tosl:terminate ;
    odrl:target :agreement01 ;
    odrl:assignee :developer ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide written notice of termination to Elsevier." ;
        odrl:action odrl:inform ;
        odrl:target :agreement01 ;
        odrl:assignee :developer ;
        odrl:informedParty :Elsevier ;
        odrl:constraint [
            a odrl:Constraint ;
            odrl:leftOperand odrl:deliveryChannel ;
            odrl:operator odrl:eq ;
            odrl:rightOperand "writing"^^xsd:string
        ]
    ] .

:permDeactivateAPIKey a odrl:Permission ;
    dcterms:description "Elsevier may deactivate the API Key; deactivation ends the Agreement." ;
    odrl:action odrl:modify ;
    odrl:target :apiKey ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:effect ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "deactivated"^^xsd:string
    ] ;
    tosl:trigger [
        a odrl:Duty ;
        dcterms:description "Upon deactivation of the API Key, terminate the Agreement." ;
        odrl:action tosl:terminate ;
        odrl:target :agreement01 ;
        odrl:assignee :Elsevier
    ] .