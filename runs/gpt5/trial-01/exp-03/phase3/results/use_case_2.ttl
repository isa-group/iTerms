@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier is the provider of the API Service Agreement referenced herein."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "The API user (you)."^^xsd:string .

:relatedTerms a odrl:Asset ;
    dcterms:description "The applicable additional terms: either the agreement between Elsevier and your institution/company/organization, or, if not applicable, the terms separately agreed between Elsevier and you for your specific use case."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "You accept and agree to the terms and conditions of this Agreement and, additionally, to the applicable terms agreed between Elsevier and your institution or, if none, the terms agreed between Elsevier and you for your specific use case."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligation01, :obligation02 ;
    odrl:assigner :Elsevier .

:obligation01 a odrl:Duty ;
    dcterms:description "You accept and agree to the terms and conditions of this Agreement." ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :customer .

:obligation02 a odrl:Duty ;
    dcterms:description "You also accept and agree to the applicable additional terms: either the institution agreement with Elsevier, or, if not applicable, the terms agreed between Elsevier and you for your specific use case." ;
    odrl:action tosl:consent ;
    odrl:target :relatedTerms ;
    odrl:assignee :customer .