@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "Developer using Elsevier APIs under this agreement."^^xsd:string .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs and related materials."^^xsd:string .

:usagePolicies a skos:Concept ;
    dcterms:description "Usage policies applicable to Elsevier APIs."^^xsd:string ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier grants a limited license to use its APIs and related materials to develop and integrate applications, subject to usage policies and written email notice requirements."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    dcterms:requires :usagePolicies ;
    odrl:permission :permission01 ;
    odrl:assigner :ELSEVIER .

:permission01 a odrl:Permission ;
    dcterms:description "Limited license to use Elsevier APIs and related materials to develop and integrate your application, subject to usage policies, with all notices delivered in writing to the email listed in the developer portal."^^xsd:string ;
    odrl:action odrl:use, tosl:develop, tosl:integrate ;
    odrl:target :apis ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Use is conditional on compliance with applicable usage policies."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :usagePolicies
    ] ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provider must deliver all notices in writing to the email listed in the developer portal."^^xsd:string ;
        odrl:action odrl:inform ;
        odrl:target :apis ;
        odrl:assignee :ELSEVIER ;
        odrl:informedParty :customer ;
        odrl:constraint [
            a odrl:Constraint ;
            odrl:leftOperand odrl:deliveryChannel ;
            odrl:operator odrl:eq ;
            odrl:rightOperand "email"^^xsd:string
        ]
    ] .