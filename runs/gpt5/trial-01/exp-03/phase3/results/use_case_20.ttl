@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Governing law and jurisdiction: Dutch law applies; courts of The Netherlands have exclusive jurisdiction, except where local law requires otherwise."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:obligation :obligation01 ;
    tosl:onDispute :litigation01 ;
    odrl:assigner :Elsevier .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier B.V., provider of the API Service."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:businessCustomer a tosl:BusinessCustomer ;
    dcterms:description "Business customer using Elsevier APIs."^^xsd:string .

:netherlandsCourts a tosl:Jurisdiction .
:netherlandsLaw a tosl:Law .

:litigation01 a tosl:Litigation ;
    dcterms:description "Disputes arising out of or pertaining to this Agreement are subject to the exclusive jurisdiction of the courts of The Netherlands and governed by the laws of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    tosl:targetParty :businessCustomer ;
    tosl:takesPlaceIn :netherlandsCourts ;
    tosl:governedBy :netherlandsLaw ;
    tosl:condition [
        a odrl:Constraint ;
        odrl:leftOperand :localLawRequirement ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "false"^^xsd:boolean
    ] .

:localLawRequirement a odrl:LeftOperand .

:obligation01 a odrl:Duty ;
    dcterms:description "The business customer irrevocably consents to the exclusive jurisdiction of the courts of The Netherlands for any action or suit arising out of or pertaining to this Agreement."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :businessCustomer .