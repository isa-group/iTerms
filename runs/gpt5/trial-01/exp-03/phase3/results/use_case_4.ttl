@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., the provider of the API Service."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier API Service Agreement governing use of Elsevier's APIs."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .

:obligation01 a odrl:Duty ;
    dcterms:description "When changes to the Agreement are made, Elsevier will make a new copy of the Agreement available on the API Service’s website." ;
    odrl:action tosl:publish ;
    odrl:target :agreement01 ;
    odrl:assignee :ELSEVIER ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Applies when changes to the Agreement have been made." ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :agreementChanged
    ] ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "The Agreement must be made available on the API Service’s website." ;
        odrl:leftOperand odrl:deliveryChannel ;
        odrl:operator odrl:eq ;
        odrl:rightOperand <https://dev.elsevier.com/api_service_agreement.html>
    ] .