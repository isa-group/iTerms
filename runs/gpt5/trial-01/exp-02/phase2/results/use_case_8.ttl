@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V., the provider of the APIs and services."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:developer a tosl:Customer ;
    dcterms:description "Developer using the Elsevier APIs and services."^^xsd:string .

:apisServices a tosl:Service ;
    dcterms:description "Elsevier APIs and any of its services."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier may block, change, suspend, remove, disable or otherwise terminate access to the APIs and services upon reasonable notice to Developer."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:assigner :ELSEVIER .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier reserves the right to block, change, suspend, remove or disable access to the APIs and services; this permission enables Elsevier to terminate access upon reasonable notice to Developer."^^xsd:string ;
    odrl:action tosl:terminate ;
    odrl:target :apisServices ;
    odrl:assignee :ELSEVIER ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide reasonable notice to Developer before terminating or disabling access."^^xsd:string ;
        odrl:action odrl:inform ;
        odrl:target :apisServices ;
        odrl:assignee :ELSEVIER ;
        odrl:informedParty :developer ;
        odrl:constraint [
            a odrl:Constraint ;
            dcterms:description "Notice must be reasonable."^^xsd:string ;
            odrl:leftOperand tosl:justification ;
            odrl:operator odrl:eq ;
            odrl:rightOperand "reasonable notice"^^xsd:string
        ]
    ] .