@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier provides the API Service under the API Service Agreement."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "This party represents the Developer using the Elsevier API Service."^^xsd:string .

:apiService a tosl:Service ;
    dcterms:description "Elsevier API Service."^^xsd:string .

:relatedAsset a odrl:LeftOperand .

:permission01 a odrl:Permission ;
    dcterms:description "The Customer may use the Elsevier API Service under this Agreement."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiService ;
    odrl:assignee :customer .

:liability01 a tosl:Liability ;
    dcterms:description "The Developer (Customer) is solely responsible and will hold Elsevier harmless for all costs, expenses, losses, and liabilities incurred, and for activities undertaken by the Developer in connection with the API Service."^^xsd:string ;
    tosl:liableParty :customer ;
    tosl:targetParty :ELSEVIER ;
    rdf:value tosl:anyLiability, tosl:anyLoss ;
    tosl:limitation [
        a odrl:Constraint ;
        odrl:leftOperand :relatedAsset ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :apiService
    ] .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Liability clause: The Developer will be solely responsible and hold Elsevier harmless for all costs, expenses, losses and liabilities incurred, and activities undertaken by the Developer in connection with the API Service."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    tosl:liability :liability01 ;
    odrl:assigner :ELSEVIER .