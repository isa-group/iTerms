@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is the Provider of the APIs and services under this Agreement."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "The Customer (developer) integrating and using Elsevier APIs."^^xsd:string .

:apiKey a odrl:Asset ;
    dcterms:description "API Key issued by Elsevier upon execution of this Agreement for the Customer's Application."^^xsd:string .

:customerApp a tosl:Service ;
    dcterms:description "The Customer's Application authorized under this Agreement."^^xsd:string .

:issuedBy a odrl:LeftOperand .
:application a odrl:LeftOperand .

:agreementExecution a odrl:Asset .

:obligation01 a odrl:Duty ;
    dcterms:description "The Customer's Application must use only the API Key issued by Elsevier upon execution of this Agreement, and no other API Key issued for other applications or developers."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiKey ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand :issuedBy ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :ELSEVIER
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:event ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :agreementExecution
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:recipient ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :customer
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand :application ;
                odrl:operator odrl:eq ;
                odrl:rightOperand :customerApp
            ]
        )
    ] .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Elsevier API Service Agreement (Clause 3.1.2)."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .