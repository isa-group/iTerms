@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "ELSEVIER is the provider of the API Service Agreement referenced herein."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "This party represents the API consumer under the Elsevier API Service Agreement."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Liability disclaimer and limitation clause extracted from the Elsevier API Service Agreement (2017)."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01 ;
    tosl:limitationOfLiability :liability01, :liability02 ;
    odrl:assigner :ELSEVIER .

:obligation01 a odrl:Duty ;
    dcterms:description "The customer consents to this Agreement as a condition of use."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:consentType ;
        odrl:operator odrl:eq ;
        odrl:rightOperand tosl:implicitConsent
    ] ;
    odrl:target :agreement01 ;
    odrl:assignee :customer .

:liability01 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, Elsevier disclaims all liability to the Customer in connection with the parties' performance of this Agreement and will not be liable for special, indirect, or consequential damages, including but not limited to loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :ELSEVIER ;
    tosl:targetParty :customer ;
    rdf:value tosl:anyLiability, tosl:anyIndirectDamage, tosl:anyLoss .

:liability02 a tosl:Liability ;
    dcterms:description "Except as expressly stated in this Agreement, the Customer disclaims all liability to Elsevier in connection with the parties' performance of this Agreement and will not be liable for special, indirect, or consequential damages, including but not limited to loss of profits, loss of business, or unfitness for user purposes."^^xsd:string ;
    tosl:liableParty :customer ;
    tosl:targetParty :ELSEVIER ;
    rdf:value tosl:anyLiability, tosl:anyIndirectDamage, tosl:anyLoss .