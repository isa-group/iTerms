@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier B.V. as the provider and rights holder."^^xsd:string .

:customer a tosl:Customer ;
    dcterms:description "Developer using Elsevier APIs under this agreement."^^xsd:string .

:apiContent a odrl:Asset ;
    dcterms:description "Content obtained via the Elsevier API."^^xsd:string .

:application a tosl:Service ;
    dcterms:description "The Developer's Application integrating the Elsevier API."^^xsd:string .

:agreement01 a odrl:Agreement ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Terms governing use of Elsevier APIs and content."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:prohibition :prohibition01 ;
    odrl:assigner :ELSEVIER .

:prohibition01 a odrl:Prohibition ;
    dcterms:description "The Developer shall not use any content obtained via the API outside the Application, unless permitted by Elsevier in writing."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apiContent ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:product ;
                odrl:operator odrl:neq ;
                odrl:rightOperand :application
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand tosl:consentType ;
                odrl:operator odrl:neq ;
                odrl:rightOperand tosl:writtenPermission
            ]
        )
    ] .