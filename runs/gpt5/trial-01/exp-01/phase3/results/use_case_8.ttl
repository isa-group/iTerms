@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "API Service Agreement terms from Elsevier."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :Elsevier ;
    dcterms:issued "2017"^^xsd:gYear ;
    odrl:permission :permission01 ;
    odrl:assigner :Elsevier .

:Elsevier a tosl:Provider ;
    dcterms:description "Elsevier B.V., the provider of the APIs and services."^^xsd:string ;
    foaf:name "Elsevier" ;
    foaf:homepage <https://www.elsevier.com/> .

:developer a tosl:Customer ;
    dcterms:description "The Developer using Elsevier APIs and services."^^xsd:string .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs."^^xsd:string ;
    odrl:partOf :apiServices .

:services a tosl:Service ;
    dcterms:description "Elsevier services."^^xsd:string ;
    odrl:partOf :apiServices .

:apiServices a odrl:Asset ;
    dcterms:description "APIs and services provided by Elsevier."^^xsd:string .

:permission01 a odrl:Permission ;
    dcterms:description "If there is a breach of this Agreement, Elsevier may block, change, suspend, remove, or disable access to the APIs and services at any time upon reasonable notice to Developer."^^xsd:string ;
    odrl:action tosl:remove ;
    odrl:target :apiServices ;
    odrl:assignee :Elsevier ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand tosl:breachOfContract
    ] ;
    odrl:duty [
        a odrl:Duty ;
        dcterms:description "Provide reasonable notice to Developer before exercising this right."^^xsd:string ;
        odrl:action odrl:inform ;
        odrl:target :apiServices ;
        odrl:assignee :Elsevier ;
        odrl:informedParty :developer
    ] .