@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "Agreement governing the use of Elsevier APIs and related materials."^^xsd:string ;
    dcterms:creator "Elsevier"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    dcterms:requires <https://dev.elsevier.com/> ;
    odrl:permission :permission01 ;
    odrl:obligation :obligation01 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Elsevier is a global information analytics business specializing in science and health."^^xsd:string ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://dev.elsevier.com/> ;
    odrl:assignerOf :agreement01 .

:customer a tosl:Customer ;
    dcterms:description "Developer using Elsevier APIs under this agreement."^^xsd:string .

:apis a tosl:Service ;
    dcterms:description "Elsevier APIs."^^xsd:string .

:materials a odrl:Asset ;
    dcterms:description "Related materials for Elsevier APIs."^^xsd:string .

:apisAndMaterials a odrl:AssetCollection ;
    dcterms:description "Elsevier APIs and related materials."^^xsd:string .

:apis odrl:partOf :apisAndMaterials .
:materials odrl:partOf :apisAndMaterials .

:permission01 a odrl:Permission ;
    dcterms:description "Elsevier grants you a limited license to use its APIs and related materials to develop and integrate your application, subject to usage policies."^^xsd:string ;
    odrl:action odrl:use ;
    odrl:target :apisAndMaterials ;
    odrl:assignee :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Use is subject to Elsevier usage policies."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:eq ;
        odrl:rightOperand <https://dev.elsevier.com/>
    ] .

:obligation01 a odrl:Duty ;
    dcterms:description "All notices will be delivered in writing to the email listed in the developer portal."^^xsd:string ;
    odrl:action odrl:inform ;
    odrl:target :agreement01 ;
    odrl:assignee :ELSEVIER ;
    odrl:informedParty :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:deliveryChannel ;
        odrl:operator odrl:eq ;
        odrl:rightOperand :developerPortalEmail
    ] .

:developerPortalEmail a skos:Concept .