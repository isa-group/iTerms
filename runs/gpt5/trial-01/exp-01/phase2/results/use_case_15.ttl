@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:obligation :obligation01, :obligation02 ;
    odrl:assigner :ELSEVIER .

:ELSEVIER a tosl:Provider ;
    foaf:name "ELSEVIER" ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "This party represents the developer using Elsevier APIs."^^xsd:string .

:elsevierTrademarks a odrl:Asset ;
    dcterms:description "Elsevier trademarks, logos, and other brand features."^^xsd:string .

:pressReleases a odrl:Asset ;
    dcterms:description "Press releases regarding this Agreement or the relationship of the parties."^^xsd:string .

:obligation01 a odrl:Duty ;
    dcterms:description "Developer must obtain written permission in advance from Elsevier to use Elsevier's trademarks, logos, or other brand features for promoting the Developer's Application."^^xsd:string ;
    odrl:action odrl:obtainConsent ;
    odrl:target :elsevierTrademarks ;
    odrl:assignee :customer ;
    odrl:consentingParty :ELSEVIER ;
    odrl:constraint [
        a odrl:LogicalConstraint ;
        odrl:and (
            [
                a odrl:Constraint ;
                odrl:leftOperand tosl:consentType ;
                odrl:operator odrl:eq ;
                odrl:rightOperand tosl:writtenPermission
            ]
            [
                a odrl:Constraint ;
                odrl:leftOperand odrl:event ;
                odrl:operator odrl:lt ;
                odrl:rightOperand odrl:policyUsage
            ]
        )
    ] .

:obligation02 a odrl:Duty ;
    dcterms:description "Any press releases regarding this Agreement or the relationship of the parties require prior approval of the parties."^^xsd:string ;
    odrl:action odrl:obtainConsent ;
    odrl:target :pressReleases ;
    odrl:assignee :customer ;
    odrl:consentingParty :ELSEVIER, :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        odrl:leftOperand odrl:event ;
        odrl:operator odrl:lt ;
        odrl:rightOperand odrl:policyUsage
    ] .