@prefix : <http://example.com/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix tosl: <https://w3id.org/tosl/> .
@base <http://example.com> .

:ELSEVIER a tosl:Provider ;
    dcterms:description "Provider of the Elsevier APIs referenced by this agreement."^^xsd:string ;
    foaf:name "ELSEVIER"^^xsd:string ;
    foaf:homepage <https://www.elsevier.com/> .

:customer a tosl:Customer ;
    dcterms:description "API client or subscriber entering into this Agreement with Elsevier."^^xsd:string .

:netherlandsLaw a tosl:Law ;
    dcterms:description "The laws of The Netherlands."^^xsd:string .

:netherlandsCourts a tosl:Jurisdiction ;
    dcterms:description "Courts of The Netherlands."^^xsd:string .

:agreement01 a odrl:Agreement, odrl:Asset ;
    odrl:profile <https://w3id.org/tosl/> ;
    dcterms:source <https://dev.elsevier.com/api_service_agreement.html> ;
    dcterms:title "API SERVICE AGREEMENT"^^xsd:string ;
    dcterms:description "This agreement is governed by the laws of The Netherlands and disputes are subject to the exclusive jurisdiction of the courts of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    dcterms:creator "ELSEVIER"^^xsd:string ;
    dcterms:rightsHolder :ELSEVIER ;
    dcterms:issued "2017-01-01"^^xsd:date ;
    odrl:permission :permission01 ;
    odrl:obligation :obligation01 ;
    tosl:onDispute :litigation01 ;
    odrl:assigner :ELSEVIER .

:permission01 a odrl:Permission ;
    dcterms:description "Customer may review this Agreement."^^xsd:string ;
    odrl:action odrl:reviewPolicy ;
    odrl:target :agreement01 ;
    odrl:assignee :customer ;
    odrl:assigner :ELSEVIER .

:obligation01 a odrl:Duty ;
    dcterms:description "The parties irrevocably consent to the exclusive jurisdiction of the courts of The Netherlands for disputes relating to this Agreement."^^xsd:string ;
    odrl:action tosl:consent ;
    odrl:target :agreement01 ;
    odrl:assignee :ELSEVIER, :customer ;
    odrl:constraint [
        a odrl:Constraint ;
        dcterms:description "Consent pertains to the exclusive forum in The Netherlands."^^xsd:string ;
        odrl:leftOperand odrl:spatial ;
        odrl:operator odrl:eq ;
        odrl:rightOperand "NL"
    ] .

:litigation01 a tosl:Litigation ;
    dcterms:description "Disputes will be governed by and construed in accordance with the laws of The Netherlands, and the parties consent to the exclusive jurisdiction of the courts of The Netherlands, except where local law requires otherwise."^^xsd:string ;
    tosl:targetParty :customer, :ELSEVIER ;
    tosl:takesPlaceIn :netherlandsCourts ;
    tosl:governedBy :netherlandsLaw ;
    tosl:condition [
        a odrl:Constraint ;
        dcterms:description "This forum applies unless local law requires a different forum."^^xsd:string ;
        odrl:leftOperand tosl:compliance ;
        odrl:operator odrl:neq ;
        odrl:rightOperand "local-law-requires-different-jurisdiction"
    ] .